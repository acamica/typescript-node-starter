{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2015",
        "outDir": "dist",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "strictNullChecks": true,
        "sourceMap": true
    }
}
